    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^17.0.2",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "@types/styled-components": "^5.1.10",
    "typescript": "^3.9.10",
    "styled-components": "^5.3.0"
